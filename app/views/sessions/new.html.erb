
<div class="jumbotron container">
	<div class="col-md-4">
		<h1>login</h1>
	</div>
	<div class="col-md-8">
		<%= bootstrap_form_for @user, {url: session_path} do |f| %>
		<p><%= f.text_field :email, placeholder: "email" %>
			<%= f.password_field :password, placeholder: "password" %>
			<%= f.submit "Log In" %></p>


	<% end %>
	</div>	
</div>
</div>